    padding: 2rpx;
    box-sizing: border-box;
}

.inner-container{
    height: 100%;
    padding-left: 15rpx;
    padding-right: 15rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background-color: #ffffff;
    font-size: 24rpx;
}

.forbidden{
    border: 2rpx dashed #bbbbbb;
}

.s-outer{
