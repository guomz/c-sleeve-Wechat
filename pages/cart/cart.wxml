<!--pages/cart/cart.wxml-->
<l-status-show show = "{{cartItems.length == 0}}" type='cart'></l-status-show>
<view wx:if="{{cartItems.length != 0}}" class="container">
    <block wx:key="index" wx:for="{{cartItems}}">
        <g-cart-item bind:overflow="onOverflow" bind:itemdelete="onDeleteItem" bind:countfloat="onCountFloat" bind:itemcheck="onSingleCheck" cart-item="{{item}}">
        </g-cart-item>
    </block>
</view>
